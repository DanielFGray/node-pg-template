{
  "name": "@danielfgray/session",
  "author": "DanielFGray <DanielFGray@gmail.com>",
  "fromEmail": "your-email@here.com",
  "projectName": "appname",
  "version": "0.1.0",
  "keywords": [],
  "license": "Unlicense",
  "description": "",
  "type": "module",
  "main": "index.js",
  "imports": {
    "#server/*": "./server/*",
    "#lib/*": "./lib/*",
    "#app/*": "./src/*"
  },
  "scripts": {
    "client:build": "vite build --ssrManifest",
    "server:build": "NODE_ENV=production esbuild --bundle --packages=external --format=esm --target=es2022 --outfile=./dist/server.js server/index.ts",
    "server:dev": "run-p start:watch 'server:build --watch'",
    "client:dev": "vite dev --clearScreen=false",
    "build": "run-s server:build client:build",
    "start:watch": "nodemon -w dist/server.js -x 'DEBUG=app:*,db:query node --env-file=.env dist/server.js'",
    "server:start": "node --env-file=.env dist/server.js",
    "start": "NODE_ENV=production run-p -s server:start worker",
    "worker": "cd worker && node --env-file=../.env ../node_modules/.bin/graphile-worker",
    "dev": "run-p -s client:dev server:dev db:watch worker",
    "env:init": "node scripts/generateEnvFile.mjs",
    "gm": "node --env-file=.env ./node_modules/.bin/graphile-migrate",
    "db:latest": "run-s -s 'gm watch --once'",
    "db:watch": "run-s -s 'gm watch'",
    "db:reset": "run-s -s 'gm reset --erase'",
    "db:up": "docker-compose up -d db",
    "db:stop": "docker-compose stop db",
    "db:exec": "docker-compose exec db",
    "db:types": "kysely-codegen --dialect postgres --log-level=info --exclude-pattern 'graphile*.*'",
    "db:ensure": "node ./scripts/dbEnsure.mjs",
    "db:setup": "node --env-file=.env ./scripts/dbSetup.mjs",
    "db:init": "run-s db:up db:setup db:reset db:latest",
    "predev": "run-s -s db:ensure",
    "init": "NOCONFIRM=1 run-s env:init db:init",
    "vitest": "node --env-file=.env ./node_modules/.bin/vitest",
    "test:units": "run-s -s 'vitest run'",
    "test:units:watch": "run-s -s 'vitest run --watch'",
    "test:dev": "run-s test:units:watch",
    "test": "run-s test:units"
  },
  "dependencies": {
    "@react-router/node": "^7.0.1",
    "@react-router/serve": "^7.0.1",
    "arctic": "^2.3.0",
    "debug": "^4.3.7",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "graphile-migrate": "^2.0.0-rc.2",
    "graphile-worker": "^0.16.6",
    "html-to-text": "^9.0.5",
    "kysely": "^0.27.4",
    "kysely-codegen": "^0.17.0",
    "lodash.template": "^4.5.0",
    "mjml": "^4.15.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.16",
    "npm-run-all": "^4.1.5",
    "oslo": "^1.2.1",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@react-router/dev": "^7.0.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/debug": "^4.1.12",
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/html-to-text": "~9.0.4",
    "@types/lodash": "~4.17.13",
    "@types/mjml": "~4.7.4",
    "@types/morgan": "^1.9.9",
    "@types/nodemailer": "~6.4.17",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.3",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "inquirer": "^12.1.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^2.1.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
