{
  "name": "@danielfgray/session",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "server:dev": "bun server/index.ts",
    "client:dev": "vite dev --clearScreen=false --open",
    "dev": "run-p -s client:dev server:dev",
    "build:client": "vite build",
    "build:server": "tsc -p server",
    "env:init": "node scripts/generateEnvFile.mjs",
    "gm": "node --env-file=.env ./node_modules/.bin/graphile-migrate",
    "db:latest": "run-s -s 'gm watch --once'",
    "db:watch": "run-s -s 'gm watch'",
    "db:reset": "run-s -s 'gm reset --erase'",
    "db:up": "docker-compose up -d db",
    "db:stop": "docker-compose stop db",
    "db:exec": "docker-compose exec db",
    "db:types": "node --env-file=.env ./node_modules/.bin/zapatos",
    "db:ensure": "node ./scripts/dbEnsure.mjs",
    "db:setup": "node --env-file=.env ./scripts/dbSetup.mjs",
    "db:init": "run-s db:up db:setup db:reset db:latest db:types",
    "predev": "run-s -s db:ensure",
    "init": "NOCONFIRM=1 run-s env:init db:init",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "DanielFGray <DanielFGray@gmail.com>",
  "license": "Unlicense",
  "description": "",
  "type": "module",
  "dependencies": {
    "argon2": "^0.28.5",
    "connect-pg-simple": "^7.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.1",
    "inquirer": "^8.2.2",
    "graphile-migrate": "^2.0.0-rc.2",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.4.0-pre.13",
    "zapatos": "^6.0.1"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.9",
    "@types/pg": "^8.6.5",
    "@types/prettier": "^2.6.3",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^8.22.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "vite": "^5.4.11"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
