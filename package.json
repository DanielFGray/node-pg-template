{
  "name": "@danielfgray/session",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "server:dev": "bun server/index.ts",
    "client:dev": "vite dev --clearScreen=false --open",
    "dev": "run-p -s client:dev server:dev",
    "build:client": "vite build",
    "build:server": "tsc -p server",
    "env:init": "node scripts/generateEnvFile.mjs",
    "gm": "node --env-file=.env ./node_modules/.bin/graphile-migrate",
    "db:latest": "run-s -s 'gm watch --once'",
    "db:watch": "run-s -s 'gm watch'",
    "db:reset": "run-s -s 'gm reset --erase'",
    "db:up": "docker-compose up -d db",
    "db:stop": "docker-compose stop db",
    "db:exec": "docker-compose exec db",
    "db:types": "node --env-file=.env ./node_modules/.bin/zapatos",
    "db:ensure": "node ./scripts/dbEnsure.mjs",
    "db:setup": "node --env-file=.env ./scripts/dbSetup.mjs",
    "db:init": "run-s db:up db:setup db:reset db:latest db:types",
    "predev": "run-s -s db:ensure",
    "init": "NOCONFIRM=1 run-s env:init db:init",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "DanielFGray <DanielFGray@gmail.com>",
  "license": "Unlicense",
  "description": "",
  "type": "module",
  "dependencies": {
    "argon2": "^0.28.7",
    "connect-pg-simple": "^10.0.0",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "graphile-migrate": "^2.0.0-rc.2",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.4.0-pre.13",
    "pg": "^8.13.1",
    "zapatos": "^6.4.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/connect-pg-simple": "~7.0.3",
    "@types/debug": "~4.1.12",
    "@types/eslint": "~9.6.1",
    "@types/eslint-plugin-jsx-a11y": "~6.10.0",
    "@types/express": "~4.17.21",
    "@types/express-session": "~1.18.0",
    "@types/morgan": "~1.9.9",
    "@types/pg": "~8.11.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "inquirer": "^12.1.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
