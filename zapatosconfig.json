{
  "db": {
    "connectionString": "{{DATABASE_URL}}"
  },
  "outDir": ".",
  "columnOptions": {
    "*": {
      "createdAt": {
        "insert": "excluded",
        "update": "excluded"
      },
      "updatedAt": {
        "insert": "excluded",
        "update": "excluded"
      }
    }
  },
  "schemas": {
    "app_private": {
      "include": "*",
      "exclude": []
    },
    "app_public": {
      "include": "*",
      "exclude": []
    },
    "graphile-worker": {
      "include": [],
      "exclude": "*"
    },
    "graphile-migrate": {
      "include": [],
      "exclude": "*"
    }
  }
}
